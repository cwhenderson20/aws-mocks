{
  "name": "aws-mocks",
  "version": "0.0.10",
  "engines": {
  	"node": ">=4"
  },
  "description": "Mocks of key AWS services for testing and local development",
  "main": "dist/index.js",
  "repository": "cwhenderson20/aws-mocks",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint 'lib/**/*.js' 'test/**/*.js'",
    "pretest": "npm run lint -s",
    "test": "npm run test:functional -s && npm run test:unit -s",
    "test:watch": "parallelshell 'npm run test:functional -s -- --watch' 'npm run test:unit -s -- --watch'",
    "test:functional": "cross-env NODE_ENV=test ava test/functional --serial --verbose",
    "test:functional:watch": "npm run test:functional -s -- --watch",
    "test:unit": "cross-env NODE_ENV=test ava test/unit --verbose",
    "test:unit:watch": "npm run test:unit -s -- --watch",
    "coverage": "nyc npm run test -s",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "author": "Chris Henderson <cwhenderson20@gmail.com>",
  "license": "MIT",
  "keywords": [
    "aws",
    "amazon",
    "mock",
    "sqs",
    "s3"
  ],
  "files": [
    "dist"
  ],
  "devDependencies": {
    "ava": "~0.16.0",
    "coveralls": "^2.11.9",
    "cross-env": "~3.1.2",
    "eslint": "~3.7.1",
    "eslint-config-airbnb": "~12.0.0",
    "eslint-plugin-import": "~2.0.0",
    "eslint-plugin-jsx-a11y": "~2.2.3",
    "eslint-plugin-node": "~2.1.2",
    "eslint-plugin-react": "~6.3.0",
    "kerberos": "0.0.21",
    "mockery": "~2.0.0",
    "nyc": "~8.3.1",
    "parallelshell": "~2.0.0",
    "pow-mongodb-fixtures": "~0.13.0",
    "rewire": "~2.5.2",
    "rimraf": "~2.5.2",
    "sinon": "~1.17.6"
  },
  "dependencies": {
    "async": "~2.0.0-rc.3",
    "debug": "~2.2.0",
    "moment": "~2.15.1",
    "mongodb": "~2.2.10",
    "mongodb-queue": "cwhenderson20/mongodb-queue#v2.0.1"
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "source": [
      "lib/**/*.js"
    ],
    "require": [
      "./lib"
    ]
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "exclude": [
      "test",
      "**/*.test.js",
      "lib/mocksConfig.js",
      "node_modules"
    ]
  }
}
